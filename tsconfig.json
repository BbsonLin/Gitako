{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "preserve",
    "strict": true,
    "lib": ["dom", "es2017.object", "es2016", "ES2020.String"],
    "baseUrl": "src",
    "outDir": "dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]
}
